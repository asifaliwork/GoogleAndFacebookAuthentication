@using GoogleAndFacebookAuthentication.Models.Account
@model IEnumerable<ApplicationUser> 
@{
    ViewData["Title"] = "USERS";
}    

<h1 class="bg-info text-white">All Users</h1>
<a asp-action="Register" class="btn btn-secondary">Create new User</a>

<table class="table table-sm table-bordered table-bordered">
    <tr><th>Name</th><th>Email</th></tr>
    @foreach (var user in Model)
    {
        <tr>
                <td width ="50%">@user.UserName</td>
                <td width="50%"> @user.Email </td>
        </tr>
    }
</table>

